{
  "name": "realtime-object-detection",
  "version": "1.0.0",
  "description": "Application web progressive de détection d'objets en temps réel utilisant TensorFlow.js et le modèle COCO-SSD, accessible depuis n'importe quel smartphone via le navigateur.",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css,md}\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w3spi5/realtime-object-detection.git"
  },
  "keywords": [
    "tensorflow",
    "object-detection",
    "coco-ssd",
    "realtime",
    "pwa",
    "web-app",
    "mobile"
  ],
  "author": "w3spi5",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/w3spi5/realtime-object-detection/issues"
  },
  "homepage": "https://github.com/w3spi5/realtime-object-detection#readme",
  "dependencies": {
    "@tensorflow-models/coco-ssd": "^2.2.3",
    "@tensorflow/tfjs": "^4.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "babel-loader": "^9.2.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.2",
    "prettier": "^3.1.1",
    "style-loader": "^4.0.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2"
  }
}
