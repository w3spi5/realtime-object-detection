
{
  "name": "realtime-object-detection",
  "version": "1.0.0",
  "description": "Application web progressive de détection d'objets en temps réel utilisant TensorFlow.js et le modèle COCO-SSD, accessible depuis n'importe quel smartphone via le navigateur.",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css,md}\"",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w3spi5/realtime-object-detection.git"
  },
  "keywords": [
    "tensorflow",
    "object-detection",
    "coco-ssd",
    "realtime",
    "pwa",
    "web-app",
    "mobile"
  ],
  "author": "w3spi5",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/w3spi5/realtime-object-detection/issues"
  },
  "homepage": "https://github.com/w3spi5/realtime-object-detection#readme",
  "dependencies": {
    "@tensorflow-models/coco-ssd": "^2.2.3",
    "@tensorflow/tfjs": "^4.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@types/jest": "^29.5.12",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.11.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^14.0.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.2",
    "prettier": "^3.1.1",
    "style-loader": "^3.3.4",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"]
  }
}
