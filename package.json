{
  "name": "realtime-object-detection",
  "version": "1.0.0",
  "description": "Application web progressive de détection d'objets en temps réel utilisant TensorFlow.js et le modèle COCO-SSD, accessible depuis n'importe quel smartphone via le navigateur.",
  "main": "src/app.js",
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md,css}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,json,md,css}\"",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.15.0",
    "@tensorflow-models/coco-ssd": "^2.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "^3.1.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}